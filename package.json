{
  "name": "store-backend-2",
  "version": "1.0.0",
  "description": "Storefront Backend",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/index.js",
    "dev": "nodemon",
    "migration:run": "db-migrate --env test up && db-migrate up",
    "migration:down": "db-migrate down",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "jasmine": "jasmine",
    "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./build --onSuccess \"node ./build/index.js\"",
    "test": "set ENV=test && db-migrate --env test up && jasmine && db-migrate db:drop devdb_test",
    "test-source": "npm run build && npm run jasmine",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mabothien/store-backend-2.git"
  },
  "author": "Tran Duy Long",
  "license": "ISC",
  "homepage": "https://github.com/mabothien/store-backend-2#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1"
  }
}
